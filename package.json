{
  "name": "global-grub-guide",
  "version": "0.0.1",
  "private": false,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run generate-dual-views && npm-run-all --parallel start:watch start:docusaurus",
    "start:docusaurus": "docusaurus start",
    "start:watch": "chokidar 'docs/food/**/*.mdx' -c 'npm run generate-dual-views'",
    "build": "npm run generate-dual-views && npm run generate-search-index && docusaurus build",
    "build:ci": "npm run generate-dual-views && npm run generate-search-index && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy:gh": "USE_SSH=true docusaurus deploy",
    "clear": "docusaurus clear",
    "clean": "rm -rf docs/cuisines-view docs/locations-view .docusaurus build",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "generate-search-index": "node scripts/generate_search_index.js",
    "generate-dual-views": "node scripts/generate_dual_views.js",
    "fetch-coordinates": "node scripts/fetch-place-coordinates-no-api.js",
    "enrich-place-ids": "node scripts/enrich-places-with-ids.js"
  },
  "dependencies": {
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@docusaurus/theme-mermaid": "^3.5.2",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@mdx-js/react": "^3.0.0",
    "@react-google-maps/api": "^2.20.7",
    "@types/react-window": "^1.8.8",
    "clsx": "^2.0.0",
    "gray-matter": "^4.0.3",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-window": "^2.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.5.2",
    "@docusaurus/tsconfig": "^3.5.2",
    "@docusaurus/types": "^3.5.2",
    "chokidar-cli": "^3.0.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  }
}
